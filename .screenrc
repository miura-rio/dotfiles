# General
defutf8         on
defencoding     utf8
encoding        utf8
startup_message off
vbell           off
msgwait         1
msgminwait      0
autodetach      on
defscrollback   5000
defbce          on
term            xterm-256color

#list window
hardstatus on
hardstatus alwayslastline "%{.Wb}.: %-w%{.RW}%n %t%{-}%+w :. %=[%Y-%m-%d %02c]"

#sehll
shell -$SHELL


